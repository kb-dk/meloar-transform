<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:gml="http://www.opengis.net/gml" xmlns:fbb="http://www.kulturarv.dk/fbb" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.kulturarv.dk/fbb" elementFormDefault="qualified" attributeFormDefault="qualified">
